?loc.begin
  brew tar

>`12,25,#rainE1,Current = @totaltime@ 
^Best = @loc.bestTime@ 
^Avg = @loc.averageTime@
//>`18,1,#rainE1,Gear score =
//^ @totalgp@

equipL vigor wand
equipR vigor shield
  
?foe = boss | 
^foe = boss_fat | foe = boss_skinny
  //>State = @foe.state@
  ?item.GetCooldown(＂bardiche＂) <= 0
  ^& foe.distance < 10
    equip bardiche
    activate R
  :?item.GetCooldown(＂mask＂) <= 0 
  ^& foe.distance < 10 & foe.armor <= 0
  ^& foe ! nagaraja
      equipR cultist mask
      activate R
  :
    equipL vigor sword
    ?foe.distance > 6
      equipR dashing shield
    ?foe.armor > 50
      equipL vigor hammer

  
?foe ! boss & foe.distance > 20
  equipL triskelion
  ?hp < maxhp
    equipL ouroboros

?foe & storage.Has(foe.name)
  var none = none
:?foe
  >Haven't killed @foe.name@ yet
  ?item.GetCooldown(＂blade＂) <= 0
    equip blade of the old god
    ?foe.distance < 12
      activate R
      storage.Set(foe.name, True)
      >Foe @foe.name@ added to kill list
      
