?item.GetCooldown(＂skeleton_arm＂) <= 0 
  equip skeleton arm
  ?buffs.string = 2
  ^& foe.distance < 8
    activate R
*/

?pickup.distance < 10
  equipL star

?hp < 15
  activate potion